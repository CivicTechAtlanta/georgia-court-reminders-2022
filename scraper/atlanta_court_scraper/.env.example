# Atlanta Court Scraper Configuration
# Copy this file to .env and fill in your values

# Base URL for the court system (optional - defaults to Atlanta)
COURT_BASE_URL=https://benchmark.atlantaga.gov

# Optional: Set logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# ChromeDriver Configuration (for Selenium-based scraping)
# Path to your ChromeDriver executable
# Leave empty to use system PATH or let selenium-manager download it automatically
CHROMEDRIVER_PATH=/Users/brentbrewington/chromedriver-mac-arm64/chromedriver

# Run Chrome in headless mode (True/False)
HEADLESS=True

# Timeout for waiting for elements (seconds)
SELENIUM_TIMEOUT=10
